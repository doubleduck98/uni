		</defaultDocument>
	</system.webServer>
	<system.web>
		<compilation debug="true" targetFramework="4.8" />
		<httpRuntime targetFramework="4.8" />
		<authentication mode="Windows" />
		<authorization>
			<deny users="?"/>
			<allow users="*"/>
		</authorization>
	</system.web>
</configuration>